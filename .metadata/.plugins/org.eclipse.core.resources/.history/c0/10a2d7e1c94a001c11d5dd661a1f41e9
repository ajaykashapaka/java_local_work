package Arrays;

public class Right_rot 
{
	public static void main(String[] args) 
	{ 
		int[] ar=new int[] {3,5,7,9,11,13};
		int i,tem;
		for(i=0;i<ar.length;i++)
		{
			System.out.print(ar[i]);
		}
		tem=ar[ar.length-1];
		for(i=ar.length-1;i>0;i--);
		{
			ar[i]=ar[i-1];
		}
		ar[i]=tem;
		System.out.println(ar[i]);
	}

}
